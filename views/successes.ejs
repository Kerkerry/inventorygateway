<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <%- include("./partials/bootstrap.ejs")  %>
</head>
<body class="bg-light">
    <%- include("./partials/navbar.ejs")  %>  
    <div class="container mt-4">
      <div class="row justify-content-center align-items-center">
          <!--Items Updated  -->
            <div class="card mx-4" style="width: 90%; padding: 0; margin-top: 5rem; margin-bottom: 5rem;">
              <div class="card-header bg-dark text-center">
                <h3 class="text-white">Last updated<span class="text-info"><%
                const updated=new Date((success[0].updated_at).toString()).toLocaleString('en-US', {
                                          weekday: 'long',
                                          year: 'numeric',
                                          month: '2-digit',
                                          day: '2-digit',
                                          hour: '2-digit',
                                          minute: '2-digit',
                                        });
                
                 %> <%=updated  %> </span> </h3>
              </div>
              <div class="card-bod">
               <div class="table-responsive">
                <table class="table table-success table-striped">
                  <thead>
                      <tr>
                        <th scope="col">#</th>
                        <th scope="col">SKU</th>
                        <th scope="col">Name</th>
                        <th scope="col">Quantity</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for (let i = 0; i < success.length; i++) {%>
                        <tr>
                              <td><%= i+1 %></td>
                              <td><%= success[i].sku%></td>
                              <td><%= success[i].name%></td>
                              <td><%= success[i].quantity%></td>
                        </tr>
                      <%} %>
                    </tbody>
                </table>
               </div>
              </div>
            </div>
      </div>
    </div>
    <%- include("./partials/jquery.ejs")  %>
</body>
</html>